datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Company {
  id          Int          @id @default(autoincrement())
  email       String       @unique
  name        String
  gridObjects GridObject[]
}

model GridObject {
  id        Int     @id @default(autoincrement())
  company   Company @relation(fields: [companyId], references: [id])
  companyId Int
  type      String
  x         Int
  y         Int
  metadata  Json
}
